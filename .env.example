# Kiwoom Securities API Configuration
KIWOOM_APP_KEY=your_app_key_here
KIWOOM_APP_SECRET=your_app_secret_here
KIWOOM_ACCOUNT_NO=your_account_number_here

# API Environment
KIWOOM_API_URL=https://api.kiwoom.com
KIWOOM_MOCK_API_URL=https://mockapi.kiwoom.com
USE_MOCK_API=false

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/kiwoom_backtest
REDIS_URL=redis://localhost:6379/0

# Application Settings
LOG_LEVEL=INFO
LOG_FILE=logs/kiwoom_backtest.log
DATA_DIR=data/
CACHE_DIR=data/cache/

# Trading Settings
DEFAULT_COMMISSION_RATE=0.00015
DEFAULT_SLIPPAGE_RATE=0.0001
MAX_POSITION_SIZE=0.95

# Performance Settings
API_RATE_LIMIT=10
API_TIMEOUT=30
CACHE_TTL=300
MAX_CONCURRENT_REQUESTS=20

# Security
JWT_SECRET_KEY=generate_a_secure_random_key_here
ENCRYPTION_KEY=generate_another_secure_key_here

# Development
DEBUG=false
TESTING=false